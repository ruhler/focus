Import('env')
#env['CFLAGS'] = ['-ggdb']
#env['CFLAGS'] = ['-pg', '-fprofile-arcs', '-ftest-coverage']
#env['LINKFLAGS'] = ['-pg', '-fprofile-arcs', '-ftest-coverage']

ccl_o = env.Object('ccl.o', 'ccl.c')

csr = env.Program('csr', ['csr.c', ccl_o], LIBS = ['SDL', 'util']);
env.Default(csr);

boxer = env.Program('boxer', ['boxer.c', ccl_o])
env.Default(boxer)

