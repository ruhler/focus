
#vim: ft=python

Import('env')

filler = env.Program('filler', 'filler.c', LIBS=['consoler'])
Default(filler)

env["filler"] = filler

fillertest = env.Program('fillertest', 'fillertest.c', LIBS=['consoler'])
Default(env.Command('fillertest.out', [fillertest, filler], "$SOURCES > $TARGET"))

env['PROGRAMDOC']('filler')
env.Install("$PREFIX/bin", filler)

env.Install("$RELEASE", [
    'filler.c',
    'filler.docl',
    'fillertest.c',
    'sconscript'
    ])


